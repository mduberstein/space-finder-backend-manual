   "version": "0.2.0",
   "configurations": [

      {
         "type": "node",
         "request": "launch",
         "name": "Launch Program",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/space-finder-backend-manual/infrastructure/Launcher.ts",
         "outFiles": [
            "${workspaceFolder}/**/*.js"
         ]
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Debug Local file",
         "runtimeArgs": [
            "-r",
            "ts-node/register"
         ],
         "args": [
            "${relativeFile}"
         ], 
         "env": {
            "AWS_REGION": "us-east-2",
            "TABLE_NAME": "SpacesTable"
         }
      }
   ]
}